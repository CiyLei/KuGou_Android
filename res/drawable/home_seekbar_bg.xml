<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    
    <item android:id="@android:id/background" >
        <shape>  
            <corners android:radius="5dp" />  
            <gradient android:startColor="@color/home_seekbar_bg" android:endColor="@color/home_seekbar_bg" />  
        </shape>  
    </item>
    <item android:id="@android:id/progress">
        <clip>  
            <shape>  
                <corners android:radius="5dp" />  
            <gradient android:startColor="@color/home_seekbar_color" android:endColor="@color/home_seekbar_color" />   
            </shape>  
        </clip>  
    </item>
</layer-list>
